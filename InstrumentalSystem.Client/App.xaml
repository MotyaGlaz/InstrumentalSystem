<Application x:Class="InstrumentalSystem.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:InstrumentalSystem.Client"
             StartupUri="View/Hub.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Dictionaries/ColorDictionary.xaml"/>
                <ResourceDictionary Source="View/Dictionaries/ImageDictionary.xaml"/>
                <ResourceDictionary Source="View/Styles/GeneralStyle.xaml"/>
                <ResourceDictionary Source="View/Styles/ProjectStructureStyle.xaml"/>
                <ResourceDictionary Source="View/Styles/ListStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
        
        
    </Application.Resources>
</Application>
